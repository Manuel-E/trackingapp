
<div class="row">
	<div class="col-sm-3 col-md-3">
		<b>Tracking Id </b>
	</div>
	<div class="col-sm-3 col-md-3"> 
		<b>Content</b>
	</div>
	<div class="col-sm-2 col-md-2"> 
		<b>State</b>
	</div>
	<div class="col-sm-2 col-md-2"> 
		<b></b>		
	</div>
	<div class="col-sm-2 col-md-2"> 
		<b></b>		
	</div>
</div>
<% @packages.each do |package|%>
<div class="row">
	<div class="col-sm-3 col-md-3">
		<%= package.tracking_id%>
	</div>
	<div class="col-sm-3 col-md-3">
		<%= package.content%>
	</div>
	<div class="col-sm-2 col-md-2"> 
		<%= package.state.name%>
	</div>

	<div class="col-sm-2 col-md-2"> 
		<%=link_to('Edit',edit_package_path(package))%>
	</div>
	<div class="col-sm-2 col-md-2">
		<%= link_to 'Delete',  package,  method: :delete, data: { confirm: 'Are you sure?' } %>
	</div>


</div>

<%end%>
